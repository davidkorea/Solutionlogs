<h1> Add a Log </h1>

<%= simple_form_for @lognote do |f| %>
<%= f.input :caseid %>
<%= f.input :title %>
<%= f.input :description %>
<%= f.input :organization %>
<%= f.input :customertype %>
<%= f.input :opco %>
<%= f.input :calldate %>
<%= f.input :handled_by %>
<%= f.input :be_resolved %>
<%= f.input :source %>
<%= f.input :attachment %>
<div class="group">
  <%= f.input :image, as: :file %>
</div>
  <%= f.submit "Submit" %>
<% end %>
